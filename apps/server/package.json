{
	"name": "server",
	"private": true,
	"scripts": {
		"build": "nest build",
		"start": "nest start",
		"dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"nest": "nest",
		"migration:generate": "pnpm run typeorm migration:generate -d ./dist/db/datasource.js ./src/db/migrations/$NAME",
